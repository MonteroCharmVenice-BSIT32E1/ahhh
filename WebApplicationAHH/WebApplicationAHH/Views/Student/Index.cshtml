@model IEnumerable<WebApplicationAHH.Models.Student>

@{
    ViewData["Title"] = "Students";
}

<h2>Students</h2>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Grade 7 Math</th>
            <th>Grade 7 Science</th>
            <th>Grade 7 English</th>
            <th>Grade 8 Math</th>
            <th>Grade 8 Science</th>
            <th>Grade 8 English</th>
            <th>Grade 9 Math</th>
            <th>Grade 9 Science</th>
            <th>Grade 9 English</th>
            <th>Grade 10 Math</th>
            <th>Grade 10 Science</th>
            <th>Grade 10 English</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Name</td>
                <td>@student.Grade7Math</td>
                <td>@student.Grade7Science</td>
                <td>@student.Grade7English</td>
                <td>@student.Grade8Math</td>
                <td>@student.Grade8Science</td>
                <td>@student.Grade8English</td>
                <td>@student.Grade9Math</td>
                <td>@student.Grade9Science</td>
                <td>@student.Grade9English</td>
                <td>@student.Grade10Math</td>
                <td>@student.Grade10Science</td>
                <td>@student.Grade10English</td>
            </tr>
        }
    </tbody>
</table>
<a asp-action="Create" class="btn btn-primary">Add New Student</a>
